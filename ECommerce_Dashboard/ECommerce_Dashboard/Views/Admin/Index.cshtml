@model IEnumerable<ECommerce_Dashboard.ViewModels.RoleVM>

<p>
    <a onclick="createRole()" class="btn btn-success">+Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.RoleName)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.RoleName)
            </td>
            <td>
                <a onclick="DeleteRole('@item.RoleName')" class="btn btn-danger">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
