{
  "version": "1.0",
  "defaultProvider": "cdnjs",
  "libraries": [
    {
      "library": "admin-lte@2.4.15",
      "destination": "wwwroot/admin-lte/"
    },
    {
      "library": "jquery@3.3.1",
      "destination": "wwwroot/lib/jquery/"
    },
    {
      "provider": "unpkg",
      "library": "bootstrap@3.4.1",
      "destination": "wwwroot/lib/bootstrap/"
    },
    {
      "library": "font-awesome@4.7.0",
      "destination": "wwwroot/lib/font-awesome/"
    },
    {
      "provider": "unpkg",
      "library": "@microsoft/signalr@latest",
      "destination": "wwwroot/js/@microsoft/signalr/"
    },
    {
      "provider": "unpkg",
      "library": "@aspnet/signalr@1",
      "destination": "wwwroot/lib/@aspnet/signalr/"
    }
  ]
}